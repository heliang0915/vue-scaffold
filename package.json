{
  "name": "vue-scaffold",
  "version": "1.0.0",
  "description": "Vue+Vuex+Vue-router+Webpack4+SSR 自定义脚手架",
  "main": "index.js",
  "scripts": {
    "startServer": "export NODE_ENV=production&&node dist/server/server.js",
    "start": "npm run build && npm run startServer",
    "deploy": "sh deploy.sh",
    "cpDir": "cp -R ./server/public/upload ./dist/server/upload &&cp -R ./server/template ./dist/server/template &&cp -R ./server/logs ./dist/server/logs &&  cp -R ./server/page ./dist/server/page",
    "buildDeploy": "rimraf dist && npm run build:client && npm run build:server&&npm run build:node&& npm run cpDir &&npm run deploy",
    "build": "rimraf dist && npm run build:client && npm run build:server&&npm run build:node&& npm run cpDir",
    "build:node": "export NODE_ENV=production&&webpack --config build/webpack.node.config.js --progress --hide-modules ",
    "build:client": "export NODE_ENV=production&&webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "export NODE_ENV=production&&webpack --config build/webpack.server.config.js --progress --hide-modules",
    "dev": "rimraf dist && npm run dev_bundle_client&&npm run dev_bundle_server",
    "dev_server": "cross-env NODE_ENV=development&& nodemon server/app.js",
    "dev_bundle_client": "cross-env NODE_ENV=development&& webpack --config build/webpack.client.config.js --watch --progress --hide-modules",
    "dev_bundle_server": "cross-env NODE_ENV=development&& webpack --config build/webpack.server.config.js --watch --progress --hide-modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heliang0915/vue-scaffold.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/heliang0915/vue-scaffold/issues"
  },
